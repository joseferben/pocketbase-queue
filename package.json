{
  "name": "pocketbase-queue",
  "version": "0.0.3",
  "description": "A type-safe queue for background tasks on top of PocketBase.",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./cjs": "./dist/index.js"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "keyword": [
    "pocketbase",
    "queue",
    "background",
    "task"
  ],
  "prettier": {
    "singleQuote": false,
    "trailingComma": "es5",
    "printWidth": 100,
    "tabWidth": 2
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json --noEmit && tsup",
    "dev": "tsx --watch text.ts",
    "format": "prettier --write --ignore-unknown .",
    "changeset": "changeset",
    "publish": "changeset version && changeset publish"
  },
  "author": "Josef Erben",
  "license": "MIT",
  "peerDependencies": {
    "pocketbase": "^0.21.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  }
}
